# 应用配置
APP_PORT=3000
APP_PREFIX=api

# 数据库配置
DB_TYPE=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=root
DB_PASSWORD=123456
DB_DATABASE=education_crm
DB_SYNCHRONIZE=true
DB_LOGGING=true

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redis123456
REDIS_DB=0

# JWT 配置
JWT_SECRET=education-crm-secret-key-2025
JWT_EXPIRES_IN=7d

# Swagger 配置
SWAGGER_ENABLE=true
SWAGGER_TITLE=教育培训CRM API
SWAGGER_DESCRIPTION=教育培训CRM系统API文档
SWAGGER_VERSION=1.0

# AI 服务配置
# DeepSeek API (主要用于文本分析)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions
DEEPSEEK_MODEL=deepseek-chat

# 豆包 API (主要用于OCR图像识别)
DOUBAO_API_KEY=your_doubao_api_key_here
DOUBAO_API_URL=https://ark.cn-beijing.volces.com/api/v3
DOUBAO_OCR_MODEL=doubao-vision-pro

# AI 配置
AI_CACHE_TTL=3600
AI_MAX_TOKENS=4000
AI_TEMPERATURE=0.3
AI_TIMEOUT=30000
