# Application Config
APP_PORT=3000
APP_PREFIX=api

# Database Config
DB_TYPE=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=root
DB_PASSWORD=123456
DB_DATABASE=education_crm
DB_SYNCHRONIZE=false
DB_LOGGING=true

# Redis Config
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redis123456
REDIS_DB=0

# JWT Config
JWT_SECRET=education-crm-secret-key-2025
JWT_EXPIRES_IN=7d

# Swagger Config
SWAGGER_ENABLE=true
SWAGGER_TITLE=Education CRM API
SWAGGER_DESCRIPTION=Education CRM System API Documentation
SWAGGER_VERSION=1.0

# AI Service Config
# DeepSeek API (for text analysis)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions
DEEPSEEK_MODEL=deepseek-chat

# Doubao API (for OCR image recognition)
DOUBAO_API_KEY=your_doubao_api_key_here
DOUBAO_API_URL=https://ark.cn-beijing.volces.com/api/v3/chat/completions
DOUBAO_ENDPOINT_ID=your_doubao_endpoint_id_here

# AI Config
AI_CACHE_TTL=3600
AI_MAX_TOKENS=4000
AI_TEMPERATURE=0.3
AI_TIMEOUT=30000
